function [ number ] = ClassToNumber( class )
    number=0;
    switch class
        case 'bark'
            number=1;
        case 'wood'
             number=2;
        case 'T03'
             number=3;
        case 'T04'
             number=4;
        case 'T05'
             number=5;
        case 'T06'
            number=6;
        case 'T07'
             number=7;
        case 'T08'
             number=8;
        case 'T09'
             number=9;
        case 'T10'
             number=10;
        case 'T11'
            number=10;
        case 'T12'
           number=10;
        case 'T13'
            number=10;
        case 'T14'
            class='brick';
        case 'T15'
            class='brick';
        case 'T16'
            class='glass';
        case 'T17'
            class='glass';
        case 'T18'
            class='carpet';
        case 'T19'
            class='carpet';
        case 'T20'
            class='upholstery';
        case 'T21'
            class='wallpaper';
        case 'T22'
            class='fur';
        case 'T23'
            class='knit';
        case 'T24'
            class='corduroy';
        case 'T25'
            class='plaid';
        otherwise
           error('da fuq?')
    end


end

